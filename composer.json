{
    "name": "seanmorris/sycamore",
    "description": "Project template for Ids projects.",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sean Morris",
            "email": "sean@seanmorr.is"
        }
    ],
    "config": {
    	"preferred-install": { "*": "source" }
    },
    "autoload": {
		"psr-4": {
			"SeanMorris\\Sycamore\\Test\\":"test/"
			, "SeanMorris\\Sycamore\\":"source/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
    "require": {
    	"php": ">=7.4",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "seanmorris/ids": "dev-sm-api-tooling as dev-master",
		"seanmorris/presskit": "dev-sm-api-tooling as dev-master",
		"seanmorris/access": "dev-sm-api-tooling as dev-master",
        "seanmorris/message" : "dev-master",
        "braintree/braintree_php" : "5.0.0"
    },
    "require-dev": {
		"simpletest/simpletest": "dev-master"
	},
	"repositories" : [{
			"type" : "vcs",
			"url" : "git@github.com:seanmorris/ids.git"
		}, {
			"type" : "vcs",
			"url" : "git@github.com:seanmorris/PressKit.git"
		}, {
			"type" : "vcs",
			"url" : "git@github.com:seanmorris/message.git"
		}, {
			"type" : "vcs",
			"url" : "git@github.com:seanmorris/access.git"
		}
	]
}
